          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import com.lemon.encryption.RSAManager;
//1:获取token
String token = vars.get(&quot;token&quot;);
log.info(&quot;------------------------&quot;);
log.info(&quot;获取到的token为：&quot;+token);

//2：获取时间戳
long timestamp = System.currentTimeMillis();
vars.put(&quot;timestamp&quot;,timestamp+&quot;&quot;);//把长整型转为字符串型
log.info(&quot;获取到的时间戳为：&quot;+timestamp);

//3：获取签名
String sign = timestamp + token;//时间戳和token拼接
log.info(&quot;拼接内容：&quot;+sign);
//RSA加密拼接的签名，得到一个加密的签名字符串
sign = RSAManager.encryptWithBase64(sign);
vars.put(&quot;sign&quot;,sign);
log.info(&quot;加密后的内容：&quot;+sign);</stringProp>
          </BeanShellPreProcessor>
